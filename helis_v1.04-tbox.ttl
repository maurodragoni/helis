:Violation rdf:type owl:Class ;
           rdfs:subClassOf [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                    owl:onProperty :hasViolationRule ;
                                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                    owl:onClass :MonitoringRule
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :hasViolationUser ;
                                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                    owl:onClass :User
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :hasViolationLevel ;
                                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                    owl:onDataRange xsd:integer
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ;
           rdfs:comment "This concept represents violations associated with a specific user. Instances of the Violation concept can be the result of reasoning activities provided by third-party applications."@en ;
           rdfs:label "Violation"@en ,
                      "Violazione"@it .


